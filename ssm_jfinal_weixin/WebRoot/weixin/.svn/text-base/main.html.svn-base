     $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
     $('.content').each(function(i){
      $(this).click(function(){
        sectionBtn(i);
        $body.animate({
          scrollTop: $('#content' + (i)).offset().top
        }, 600);
        return false;
      });
     });

});

//backtop 隐藏判断事件
function scrollWindow() {
	var navH = $('.nav').height();
	if ($(window).scrollTop() > navH) {
		$(".TopBtn").fadeIn();
	} else {
		$(".TopBtn").fadeOut();
	}
}

function scrollPage(){
